# Database (Neon with pooled + direct URLs)
DATABASE_URL=postgresql://user:password@ep-xxx-pooler.region.aws.neon.tech/dbname?sslmode=require
DATABASE_URL_UNPOOLED=postgresql://user:password@ep-xxx.region.aws.neon.tech/dbname?sslmode=require

# AI Providers
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# AI Gateway (optional, falls back to direct providers)
AI_GATEWAY_API_KEY=

# LLM Observability
LANGFUSE_SECRET_KEY=sk-lf-...
LANGFUSE_PUBLIC_KEY=pk-lf-...
LANGFUSE_BASE_URL=https://cloud.langfuse.com

# Session Secret (for iron-session, min 32 chars)
SESSION_SECRET=your-secret-key-at-least-32-characters-long

# Owner Passphrase (set during first setup)
OWNER_PASSPHRASE=

# Feature Flags
ALLOW_VISITOR_MODE=true

# Cross-Agent Integration (future - Overseer)
# OVERSEER_URL=
# OVERSEER_JWT_SECRET=

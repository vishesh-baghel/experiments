{
  "name": "worklog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "worklog": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "build": "tsup src/cli.ts --format esm --dts",
    "start": "node dist/cli.js",
    "ingest": "tsx src/cli.ts ingest",
    "test": "vitest run --exclude __tests__/e2e-real.test.ts",
    "test:watch": "vitest --exclude __tests__/e2e-real.test.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ai": "^6.0.49",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "dotenv": "^17.2.3",
    "tsup": "^8.3.5",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
